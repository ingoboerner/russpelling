   "source": [
    "#Іі Ѣѣ Ѳѳ Ѵѵ\n",
    "\n",
    "def normalizeRus(inputtext):\n",
    "    oldstring = \"ІіѢѣѲѳѴѵ\"\n",
    "    newstring = \"ИиЕеФфИи\"\n",
    "    translationtable = str.maketrans(oldstring, newstring)\n",
    "    return inputtext.translate(translationtable)\n",
    "    \n",
    "    \n",
    "\n",
    "print(normalizeRus(\"БѢЛАЯ АКАЦІЯ\"))"
   ]
